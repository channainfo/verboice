= render 'shared/project_tabs_and_title', :project => @project

= render 'form'

= link_to 'Download call results as CSV', project_call_flow_path(@project, @call_flow, :format => :csv), :class => "button fimport"

= form_tag export_call_flow_project_call_flow_path(@project, @call_flow), :method => :get do
  %button.fimport Export call Flow
  = check_box_tag :export_audios
  Export audio recordings also
%hr

Import Call Flow:
= form_tag({:action => :import_call_flow}, :multipart => true) do
  = file_field_tag 'vrb', :label => 'Browse'
  = submit_tag 'Upload', :class => 'grey-button shadow'
