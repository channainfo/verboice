<%= link_to 'Create new queue', new_call_queue_path %>

<table class="GralTable">
  <tr>
    <th>Name</th>
    <th></th>
  </tr>
<%- call_queues.each do |queue| %>
  <tr>
    <td><%= queue.name %></td>
    <td>
      <%= link_to 'view', queue %>
      <%= link_to 'edit', edit_call_queue_path(queue) %>
      <%= link_to 'delete', queue, :confirm => "Are you sure you want to delete the call queue #{queue.name}?", :method => :delete %>
    </td>
  </tr>
<%- end %>
</table>